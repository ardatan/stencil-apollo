{
  "name": "stencil-apollo",
  "version": "0.1.5",
  "description": "Stencil Apollo",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "jsnext:main": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "webComponents": "dist/web-components.json",
  "private": false,
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "build-docs": "stencil build --docs-json",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@stencil/state-tunnel": "1.0.1"
  },
  "peerDependencies": {
    "apollo-client": "^2.4.13",
    "graphql": "^14.1.1 || ^15.0.0",
    "graphql-tag": "^2.10.1"
  },
  "devDependencies": {
    "@apollo/client": "^3.2.2",
    "@stencil/core": "1.12.7",
    "graphql": "15.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ardatan/stencil-apollo.git"
  },
  "author": "Arda TANRIKULU",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ardatan/stencil-apollo"
  },
  "homepage": "https://github.com/ardatan/stencil-apollo",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
